class Supervisor
{
    isA Persona;
}

class Acto
{
  
  nombre;
  lugar;
  Double costoTotal;
  TipoActo tipo;
  1 -- 1 Empresa;
  * -> 1 TipoActo;
}

class Empresa
{
  descripcion;
  tipo;
  Double ruc;
}

class Persona
{
  nombre;
  1 -- * Acto;
}

class TipoActo
{
  codigo;
  descripcion;
  key {codigo}
}
